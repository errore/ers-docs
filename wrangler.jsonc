{
  "$schema": "./node_modules/wrangler/config-schema.json",
	"main": "dist/_worker.js/index.js",
	"name": "ers-docs",
  "compatibility_date": "2025-12-29",
  "compatibility_flags": [
    "nodejs_compat",
    "global_fetch_strictly_public"
  ],
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist"
	},
	"observability": {
    "enabled": true
  },
  "routes": [
    {
      "pattern": "ers.errore.ink",
      "custom_domain": true
    }
  ]
}